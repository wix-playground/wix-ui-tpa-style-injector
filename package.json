{
  "name": "wix-ui-tpa-style-injector",
  "version": "1.0.0",
  "description": "Provides style injection and update capability for Wix UI TPA",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run lint && npm run test && npm run build && npm run docs",
    "test": "./node_modules/.bin/jest --forceExit --detectOpenHandles",
    "testSync": "./node_modules/.bin/jest --runInBand --forceExit --detectOpenHandles",
    "lint": "./node_modules/.bin/tslint -c ./tslint.json -p ./tsconfig.json -t stylish ./lib/**/*.ts",
    "build": "./node_modules/.bin/webpack",
    "buildDev": "./node_modules/.bin/webpack --config webpack.config.dev.js",
    "docs": "./node_modules/.bin/typedoc --module commonjs --target ES6 --exclude \"**/*.test.*\" --ignoreCompilerErrors --out ./dist/docs --readme ./README.md --mode file ./index.ts ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wix-playground/wix-ui-tpa-style-injector.git"
  },
  "keywords": [
    "Wix",
    "UI",
    "TPA",
    "connected",
    "style",
    "injector"
  ],
  "author": "Arturas Chramcenko",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "npm-dts-webpack-plugin": "^1.1.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "find-node-modules": "^2.0.0"
  }
}
